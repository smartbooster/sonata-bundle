<?xml version="1.0"?>
<xliff version="1.2" xmlns="urn:oasis:names:tc:xliff:document:1.2">
    <file source-language="fr" datatype="plaintext" original="file.ext">
        <body>
            <!-- Smart Email translation -->
            <trans-unit id="smart.email.general_info">
                <source>smart.email.general_info</source>
                <target><![CDATA[
                    <p>This page is used to validate the content and design of emails sent.</p>
                    <p>Since these emails are dynamic and context-dependent, the action buttons are fake and the dynamic parameters are displayed between percent sign, like this %param%.</p>
                    <p>You will find the description of the functionalities of the buttons/link directly integrated at the bottom of each email.</p>
                ]]></target>
            </trans-unit>
            <trans-unit id="smart.email.subject">
                <source>smart.email.subject</source>
                <target>Subject</target>
            </trans-unit>
            <trans-unit id="smart.email.recipient">
                <source>smart.email.recipient</source>
                <target>Recipient</target>
            </trans-unit>
            <trans-unit id="smart.email.test_form_help">
                <source>smart.email.test_form_help</source>
                <target>Send this email to check his deliverability</target>
            </trans-unit>
            <trans-unit id="smart.email.test_form.email_error">
                <source>smart.email.test_form.email_error</source>
                <target><![CDATA[
                    Invalid email <b>%recipient%</b>. The email has not been sent.
                ]]></target>
            </trans-unit>
            <trans-unit id="smart.email.test_form.success">
                <source>smart.email.test_form.success</source>
                <target><![CDATA[
                    Email <b>#%code%</b> sent to <b>%recipient%</b>
                ]]></target>
            </trans-unit>
            <trans-unit id="smart.email.empty_recipient_error">
                <source>smart.email.empty_recipient_error</source>
                <target>Undefined recipient for email with code %code%</target>
            </trans-unit>
            <trans-unit id="smart.email.missing_smart_group">
                <source>smart.email.missing_smart_group</source>
                <target>Missing email group translation</target>
            </trans-unit>
            <trans-unit id="smart.email.missing_title">
                <source>smart.email.missing_title</source>
                <target>Missing email title translation</target>
            </trans-unit>
            <trans-unit id="smart.email.missing_subject">
                <source>smart.email.missing_subject</source>
                <target>Missing email subject translation</target>
            </trans-unit>
            <trans-unit id="smart.email.missing_template">
                <source>smart.email.missing_template</source>
                <target>Missing email template</target>
            </trans-unit>
            <trans-unit id="email.smart_group_admin">
                <source>email.smart_group_admin</source>
                <target>Backoffice</target>
            </trans-unit>

            <trans-unit id="admin.security.forgot_password.subject">
                <source>admin.security.forgot_password.subject</source>
                <target>Forgot password request</target>
            </trans-unit>
            <trans-unit id="admin.security.forgot_password.doc_title">
                <source>admin.security.forgot_password.doc_title</source>
                <target>Reinit Backoffice password</target>
            </trans-unit>
            <trans-unit id="reset_password.link.documentation_help">
                <source>reset_password.link.documentation_help</source>
                <target><![CDATA[
                    Link <b>%action%</b> : Returns to the backoffice url /reset_password so that the admin can update his password
                ]]></target>
            </trans-unit>
            <trans-unit id="forgot_password.welcome">
                <source>forgot_password.welcome</source>
                <target><![CDATA[
                    Hello,<br />
                    <br />
                    You have made a request to reset your password.<br />
                    If you didn't request it, you can just ignore this email.
                ]]></target>
            </trans-unit>
            <trans-unit id="forgot_password.action.click">
                <source>forgot_password.action.click</source>
                <target>To reset your password click on the link below :</target>
            </trans-unit>
            <trans-unit id="forgot_password.reset_password.link">
                <source>forgot_password.reset_password.link</source>
                <target>Reset my password</target>
            </trans-unit>

            <trans-unit id="admin.security.account_creation.subject">
                <source>admin.security.account_creation.subject</source>
                <target>To complete your account, please initialize your password</target>
            </trans-unit>
            <trans-unit id="admin.security.account_creation.doc_title">
                <source>admin.security.account_creation.doc_title</source>
                <target>Finish Backoffice account creation</target>
            </trans-unit>
            <trans-unit id="account_creation.welcome">
                <source>account_creation.welcome</source>
                <target><![CDATA[
                    Hello,
                ]]></target>
            </trans-unit>
            <trans-unit id="account_creation.action.click">
                <source>account_creation.action.click</source>
                <target>To finalize the creation of your account, please follow the link to initialize your password:</target>
            </trans-unit>
            <trans-unit id="account_creation.reset_password.link">
                <source>account_creation.reset_password.link</source>
                <target>Initialize my password</target>
            </trans-unit>
            <trans-unit id="security.token.information">
                <source>security.token.information</source>
                <target>Please note, for security reasons, these links are for single use only. Please delete these emails once the action has been performed.</target>
            </trans-unit>
        </body>
    </file>
</xliff>

<?xml version="1.0"?>
<xliff version="1.2" xmlns="urn:oasis:names:tc:xliff:document:1.2">
    <file source-language="fr" datatype="plaintext" original="file.ext">
        <body>
            <!-- ImportType -->
            <trans-unit id="import_type.no_data_error">
                <source>import_type.no_data_error</source>
                <target>No data entered.</target>
            </trans-unit>
            <trans-unit id="import_type.not_unique_data_source_error">
                <source>import_type.not_unique_data_source_error</source>
                <target>There can only be one data source. Please choose between the csv upload or the textarea.</target>
            </trans-unit>
            <!-- ArrayUtils -->
            <trans-unit id="array_utils.multi_array_nb_max_rows_error">
                <source>array_utils.multi_array_nb_max_rows_error</source>
                <target>The maximum number of %nbMaxRows% rows has been exceeded (%nbRows% rows detected during processing).</target>
            </trans-unit>
            <trans-unit id="array_utils.multi_array_header_consistency_error">
                <source>array_utils.multi_array_header_consistency_error</source>
                <target>The number of expected columns per row does not match. Please check the delimiter as well as the data for the following rows: {keys}</target>
            </trans-unit>
            <!-- Import Loader -->
            <trans-unit id="smart_etl.loader.load_unvalid_objects_error">
                <source>smart_etl.loader.load_unvalid_objects_error</source>
                <target>Some data values to import contains errors, please fix them as describe bellow, then you can import again.</target>
            </trans-unit>
            <trans-unit id="smart_etl.loader.error_detected_on_line">
                <source>smart_etl.loader.error_detected_on_line</source>
                <target>%nb_error% Errors detected on line %nb_line%</target>
            </trans-unit>
            <trans-unit id="smart_etl.loader.available_choices">
                <source>smart_etl.loader.available_choices</source>
                <target>Available choices : </target>
            </trans-unit>
            <trans-unit id="smart_sonata.import.denormalize_error">
                <source>smart_sonata.import.denormalize_error</source>
                <target>Typing for some data values are wrong, please fix them as describe bellow, then you can import again.</target>
            </trans-unit>
        </body>
    </file>
</xliff>

                <target>Création</target>
            </trans-unit>
            <trans-unit id="import.diff_type_change">
                <source>import.diff_type_change</source>
                <target>Mise à jour</target>
            </trans-unit>
            <trans-unit id="import.diff_type_unchanged">
                <source>import.diff_type_unchanged</source>
                <target>Aucun changement</target>
            </trans-unit>
